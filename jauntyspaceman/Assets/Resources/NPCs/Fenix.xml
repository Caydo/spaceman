<npc name="Fenix">
<spriteset file="Male.png"/>
<dialog>
<point id="0" text="En Taro Adun!" color="#00ff00ff">
<response key="1" text="My life for Aiur!" trigger="Go To 1"/>
<response key="2" text="En Taro your face!" trigger="Go To 2"/>
<response key="3" text="Clearly, Tassadar has failed us." trigger="Go To 2"/>
<response key="(timeout)" text="" trigger="Go To 2"/>
</point>
<point id="1" text="What are your orders, Executor?">
<response key="1" text="Construct additional pylons." trigger="Go To 3"/>
<response key="2" text="We require more vespene gas." trigger="Go To 3"/>
<response key="3" text="There is no cow level." trigger="Go To 2"/>
<response key="(timeout)" text="" trigger="Go To 2"/>
</point>
<point id="2" text="I didn't come back for this…" trigger="Take 0.5 O2, EndDialog">
</point>
<point id="3" text="As you command." trigger="Give 0.1 O2, EndDialog">
</point>
</dialog>
</npc>
